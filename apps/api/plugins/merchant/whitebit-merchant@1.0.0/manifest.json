{
  "name": "whitebit-merchant",
  "type": "merchant",
  "version": "1.0.0",
  "timeouts": { "callMs": 15000, "bootMs": 5000 },
  "net": { "egressAllowList": ["whitebit.com"] },
  "allowCurrencyCodes": ["USDTTRC20", "BNBBEP20"],
  "webhook": {
      "supported": true,
      "endpoint": "whiteBIT-verification"
  },
  "configSchema": {
      "type": "object",
      "additionalProperties": false,
      "required": ["publicKey", "secretKey", "webhookKey"],
      "properties": {
          "webhookKey": {
              "type": "string",
              "minLength": 10
          },
          "publicKey": {
              "type": "string",
              "minLength": 10
          },
          "secretKey": {
              "type": "string",
              "minLength": 10
          }
      }
  }
}
